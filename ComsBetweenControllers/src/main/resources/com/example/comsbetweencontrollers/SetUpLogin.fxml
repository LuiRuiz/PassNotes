<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="firstLogin" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comsbetweencontrollers.LoginController">
   <children>
      <PasswordField fx:id="loginField" layoutX="285.0" layoutY="218.0" promptText="Enter Here" />
      <Label layoutX="162.0" layoutY="221.0" text="Initialize Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="PasswordErrorString" layoutX="170.0" layoutY="305.0" opacity="0.0" text="WEAK PASSWORD: Please enter a valid password" textFill="#ff0101" underline="true" />
      <Button fx:id="loginButton" layoutX="262.0" layoutY="264.0" mnemonicParsing="false" onAction="#pressLogin" text="Login" />
   </children>
</AnchorPane>
